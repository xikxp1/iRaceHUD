{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../build",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "category": "Utility",
    "targets": "nsis",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "iracehud",
  "mainBinaryName": "iracehud",
  "version": "0.0.0",
  "identifier": "dev.xikxp1.iracehud",
  "plugins": {},
  "app": {
    "windows": [
      {
        "label": "main",
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "resizable": false,
        "maximizable": false,
        "minimizable": false,
        "closable": false,
        "fullscreen": true,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  }

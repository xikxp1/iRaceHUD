{
  "identifier": "desktop-capability",
  "local": true,
  "platforms": [
    "windows"
  ],
  "windows": [
    "main",
    "settings"
  ],
  "permissions": [
    "core:default",
    "updater:default",
    "autostart:default",
    "store:default",
    "global-shortcut:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://github.com/xikxp1/iRaceHUD"
        }
      ]
    }
  ]
}

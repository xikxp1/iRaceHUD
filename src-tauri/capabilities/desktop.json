{
  "identifier": "desktop-capability",
  "local": true,
  "platforms": [
    "windows"
  ],
  "windows": [
    "main",
    "telemetry",
    "timer",
    "subtimer",
    "proximity",
    "standings",
    "track_map",
    "lap_times",
    "relative",
